<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="customer_from_view">
        <field name="name">customer.from.view</field>
        <field name="model">customer</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                        </group>
                        <group>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record model="ir.ui.view" id="customer_tree_view">
        <field name="name">customer.tree.view</field>
        <field name="model">customer</field>
        <field name="arch" type="xml">
            <tree>
            </tree>
        </field>
    </record>

    <record id="customer_action" model="ir.actions.act_window">
        <field name="name">customer</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">customer</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="car_rental_menu"
              name="Car Rental"
              sequence="1"/>

    <menuitem id="customer_menu"
              name="Customer"
              parent="car_rental_menu"
              sequence="10"/>

    <menuitem id="customer_sub_menu"
              name="Customer"
              parent="customer_menu"
              action="customer_action"
              sequence="10"/>

</odoo>