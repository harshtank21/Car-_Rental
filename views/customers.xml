<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="customer_from_view">
        <field name="name">customer.from.view</field>
        <field name="model">customer</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <!--                            <button class="oe_stat_button" type="object" name="update_car" string="update car" />-->
                            <field name="name"/>
                            <field name="address"/>
                            <field name="star_date"/>
                            <field name="licence"/>
                            <field name="licence_attach"/>
                        </group>
                        <group>
                            <field name="end_date"/>
                            <field name="rent_time"/>
                            <field name="identity"/>
                            <field name="Identity_img"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record model="ir.ui.view" id="customer_tree_view">
        <field name="name">customer.tree.view</field>
        <field name="model">customer</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="customer_action" model="ir.actions.act_window">
        <field name="name">customer</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">customer</field>
        <field name="view_mode">tree,form</field>
    </record>


</odoo>