<odoo>
    <record model="ir.ui.view" id="new_order_from_view">
        <field name="name">new.order.from.view</field>
        <field name="model">new.order</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                        </group>
                        <group>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record model="ir.ui.view" id="new_order_tree_view">
        <field name="name">new.order.tree.view</field>
        <field name="model">new.order</field>
        <field name="arch" type="xml">
            <tree>
            </tree>
        </field>
    </record>

    <record id="new_order_action" model="ir.actions.act_window">
        <field name="name">New Order</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">new.order</field>
        <field name="view_mode">tree,form</field>
    </record>


    <menuitem id="order_menu"
              name="Order"
              parent="car_rental_menu"
              sequence="10"/>

    <menuitem id="customer_sub_menu"
              name="New Order"
              parent="order_menu"
              action="new_order_action"
              sequence="10"/>

</odoo>